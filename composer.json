{
    "name": "undefined/translate-locallang",
    "type": "typo3-cms-extension",
    "version": "2.9.0",
    "description": "Backend Module for creating and editing of language files (locallang.xlf).",
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": ">=11.5.0 <=12.4.99",
        "typo3/cms-extbase": ">=11.5.0 <=12.4.99",
        "typo3/cms-backend": ">=11.5.0 <=12.4.99",
        "typo3/cms-extensionmanager": ">=11.5.0 <=12.4.99"
    },
    "replace": {
        "typo3-ter/translate-locallang": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Undefined\\TranslateLocallang\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "translate_locallang"
        }
    },
    "config": {
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true
        }
    }
}
